@using System
@using System.Linq
@using System.Collections.Generic
@using Nancy.Security
@using SmartHomeWeb
@using SmartHomeWeb.Model
@using Nancy.ViewEngines.Razor
@* Model is a (SensorId, StartTime, EndTime, MaxMeasurements) tuple *@
@inherits NancyRazorViewBase<AutofitRange>

@Html.Partial("graph.cshtml")

<script>
    measurementsGraph.setData(@Model.SensorId, new Date("@Model.StartTime"), new Date("@Model.EndTime"), @Model.MaxMeasurements);
</script>
